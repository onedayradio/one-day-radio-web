{
  "name": "one-day-radio-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.6",
    "@apollo/link-context": "2.0.0-beta.3",
    "@chakra-ui/react": "1.0.3",
    "@emotion/react": "11.1.2",
    "@emotion/styled": "11.0.0",
    "@types/node": "14.14.13",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "easy-peasy": "4.0.1",
    "framer-motion": "^2.9.5",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "query-string": "6.13.7",
    "react": "17.0.1",
    "react-device-detect": "1.14.0",
    "react-dom": "17.0.1",
    "react-icons": "4.1.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-slick": "0.27.13",
    "slick-carousel": "1.8.1",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "cross-env PORT=3001 env-cmd -f .env react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.development react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-all": "yarn upgrade-interactive --latest",
    "format": "prettier --config .prettierrc 'src/**/*.(ts|tsx)' --write",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn format && yarn lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": "**/*.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "5.1.6",
    "@types/react-slick": "0.23.4",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "husky": "4.3.5",
    "prettier": "2.2.1"
  }
}
