{
  "name": "one-day-radio-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.15",
    "@apollo/link-context": "2.0.0-beta.3",
    "@chakra-ui/react": "1.6.0",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.3.0",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "easy-peasy": "5.0.3",
    "framer-motion": "4.1.9",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "query-string": "7.0.0",
    "react": "17.0.2",
    "react-device-detect": "1.17.0",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-icons": "4.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-slick": "0.28.1",
    "slick-carousel": "1.8.1",
    "typescript": "4.2.4"
  },
  "scripts": {
    "start": "cross-env PORT=3001 env-cmd -f .env react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.development react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-all": "yarn upgrade-interactive --latest",
    "prettier": "prettier --config .prettierrc 'src/**/*.(ts|tsx)' --write",
    "pretty": "pretty-quick --config .prettierrc --write 'src/**/*.(ts|tsx)'",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --quiet",
    "lint-fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/react-router-dom": "5.1.7",
    "@types/react-slick": "0.23.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "commitizen": "^4.2.3",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "env-cmd": "10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "6.0.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "pretty-quick": "^3.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
